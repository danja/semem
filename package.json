{
  "name": "semem",
  "version": "1.1.0",
  "description": "Semantic Memory",
  "author": "Danny Ayers <danny.ayers@gmail.com> (https://danny.ayers.name)",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "start": "node servers/start-all.js",
    "start:dev": "npm run build:dev && node servers/start-all.js",
    "stop": "pkill -f 'node servers/start-all\\.js' || true; pkill -f 'webpack.*serve' || true",
    "mcp:http": "node src/servers/mcp-server-fixed.js http",
    "mcp:stdio": "node src/servers/mcp-server-fixed.js stdio",
    "mcp:sse": "node src/servers/mcp-server-fixed.js sse",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:watch": "webpack --mode=development --watch",
    "dev": "webpack serve --mode=development",
    "test": "vitest run --run --exclude=**/*.llm.* --exclude=**/llms/** --exclude=**/Ollama* --exclude=**/embeddings/** --exclude=**/search/SearchService.test.js",
    "test:llms": "vitest run --run --include=**/*.llm.* **/llms/** **/Ollama* **/embeddings/** **/search/SearchService.test.js",
    "test:mocked": "./run-mocked-tests.sh",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage --exclude=**/*.llm.* --exclude=**/llms/** --exclude=**/Ollama* --exclude=**/embeddings/** --exclude=**/search/SearchService.test.js",
    "coverage:mocked": "./run-mocked-tests.sh --coverage",
    "test:ui": "vitest --ui",
    "test:ci": "./run-mocked-tests.sh --coverage --reporter=json --outputFile=./coverage/coverage.json",
    "test:report": "vitest run --coverage --reporter=html --exclude=**/*.llm.* --exclude=**/llms/** --exclude=**/Ollama* --exclude=**/embeddings/** --exclude=**/search/SearchService.test.js",
    "test:check": "vitest run --coverage --exclude=**/*.llm.* --exclude=**/llms/** --exclude=**/Ollama* --exclude=**/embeddings/** --exclude=**/search/SearchService.test.js",
    "test:llms:coverage": "vitest run --coverage --include=**/*.llm.* **/llms/** **/Ollama* **/embeddings/** **/search/SearchService.test.js",
    "coverage:badge": "coverage-badge-creator",
    "badge:create": "npm run test:coverage:mocked && coverage-badge-creator && echo 'Badge created in coverage/'",
    "docs": "jsdoc -c jsdoc.json",
    "rp": "repomix -c repomix.config.json . && repomix -c repomix-docs.config.json . ",
    "mcp-server": "node src/servers/mcp-server-fixed.js",
    "mcp-example": "node examples/MCPClientExample.js",
    "mcp-test": "node test-mcp.js",
    "tsc": "tsc --noEmit",
    "types:check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danja/semem.git"
  },
  "keywords": [
    "semantic",
    "memory",
    "llm",
    "rdf",
    "sparql"
  ],
  "bugs": {
    "url": "https://github.com/danja/semem/issues"
  },
  "homepage": "https://github.com/danja/semem#readme",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.8",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "babel-loader": "^9.2.1",
    "codecov": "^3.8.2",
    "coverage-badge-creator": "^1.0.19",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "jsdoc": "^4.0.4",
    "jsdom": "^26.1.0",
    "nyc": "^15.1.0",
    "style-loader": "^4.0.0",
    "typescript": "^5.4.2",
    "vitest": "^3.1.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.36.3",
    "@langchain/core": "^0.3.19",
    "@langchain/openai": "^0.3.14",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "faiss-node": "^0.5.1",
    "graphology": "^0.25.4",
    "helmet": "^8.0.0",
    "hyperdata-clients": "^0.10.4",
    "loglevel": "^1.9.2",
    "ml-kmeans": "^6.0.0",
    "node-fetch": "^3.3.2",
    "ollama": "^0.5.10",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "yargs": "^17.7.2",
    "zod": "^3.25.49",
    "zod-to-json-schema": "^3.24.5"
  }
}