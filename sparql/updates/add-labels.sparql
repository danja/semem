# Add rdfs:label and skos:prefLabel to an entity
# Used by augment label operation to add keyword-generated labels

PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

INSERT DATA {
  GRAPH <{{graphURI}}> {
    <{{entityURI}}> rdfs:label "{{label}}" .
    <{{entityURI}}> skos:prefLabel "{{label}}"@en .
  }
}
