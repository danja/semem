{
    "output": {
        "filePath": "./repomix-semem-docs.md",
        "headerText": "Semem docs",
        "removeComments": false
    },
    "include": [
        "**/*.md",
        "**/*.mermaid",
        "description-latest"
    ],
    "ignore": {
        "useDefaultPatterns": false,
        "customPatterns": [
            "docs",
            "data",
            ".nyc_output",
            ".env",
            "**/_*",
            "node_modules",
            "*.log",
            "**/*repomix*.txt",
            "**/*repomix*.md",
            "**/*.html",
            "**/data/*",
            "**/*copy*.js",
            "**/conversations.json"
        ]
    }
}